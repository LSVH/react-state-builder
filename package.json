{
  "name": "react-state-builder",
  "version": "0.0.1",
  "description": "A simple framework for building your application state",
  "author": "LSVH",
  "license": "MIT",
  "repository": "LSVH/react-state-builder",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
