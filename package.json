    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "cross-env": "^6.0.3",
    "gh-pages": "^2.1.1",
    "react": "^16.4.1",
    "react-docgen-typescript-loader": "^3.3.0",
